{
  "name": "@cereon/recharts",
  "version": "0.1.0",
  "description": "Reusable React dashboard chart card components for @cereon/dashboard built using Recharts library.",
  "type": "module",
  "keywords": [
    "react",
    "dashboard",
    "ui",
    "components",
    "cards",
    "recharts",
    "cereon-dashboard"
  ],
  "author": "Aditya Mishra <adimis.sde@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/adimis-ai/cereon-recharts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adimis-ai/cereon-recharts.git"
  },
  "bugs": {
    "url": "https://github.com/adimis-ai/cereon-recharts/issues"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "build:tsc": "tsc -p tsconfig.build.json",
    "build:bundle": "tsup",
    "build": "npm run clean && npm run build:bundle && npm run build:tsc",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --max-warnings 0",
    "test": "vitest",
    "prepare": "npm run build",
    "typecheck": "tsc --noEmit",
    "install": "npm run build"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "framer-motion": ">=10.0.0",
    "react": ">=16",
    "react-dom": ">=16",
    "react-grid-layout": ">=1.0.0"
  },
  "peerDependenciesMeta": {
    "framer-motion": {
      "optional": false
    },
    "react-grid-layout": {
      "optional": false
    }
  },
