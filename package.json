    "@types/react-dom": ">=16",
    "@types/react-grid-layout": "^1.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.50.0",
    "react": ">=16",
    "react-dom": ">=16",
    "rimraf": "^5.0.0",
    "tailwindcss": "^4.0.8",
    "tsup": "^6.6.0",
    "typescript": "^5.9.2",
    "vite": "^6.2.0",
    "vitest": "^1.0.0"
  }
}
